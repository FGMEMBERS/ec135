  <object-name>reardoor_re</object-name>
  <object-name>reardoor_re.i</object-name>
    <property>/sim/model/door-positions/rearDoors/position-norm</property>
  <factor>100</factor>
  
  <axis>
   <x1-m>0.072</x1-m>
   <y1-m>0.789</y1-m>
   <z1-m>1.348</z1-m>
   <x2-m>0.384</x2-m>
   <y2-m>0.788</y2-m>
   <z2-m>1.674</z2-m>
   </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
   <object-name>reardoor_li</object-name>
  <object-name>reardoor_li.i</object-name>
    <property>/sim/model/door-positions/rearDoors/position-norm</property>
  <factor>-100</factor>
  
  <axis>
   <x1-m>0.072</x1-m>
   <y1-m>-0.789</y1-m>
   <z1-m>1.348</z1-m>
   <x2-m>0.384</x2-m>
   <y2-m>-0.788</y2-m>
   <z2-m>1.674</z2-m>
   </axis>
 </animation>
 
<!--sideward animation backdoor right--> 
	<animation>
		<type>translate</type>
    		<object-name>backdoor_re_o</object-name>
		<object-name>backdoor_re_o.i</object-name>
		<object-name>backdoor_re_u</object-name>
		<object-name>backdoor_re_u.i</object-name>
		<object-name>backdoorglas_re</object-name>
<property>/sim/model/door-positions/ribackDoor/position-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>.1</ind>
				<dep>.05</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>.05</dep>
			</entry>
		</interpolation>
		<axis>
			<x>01</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
  
<!--forward animation backdoor right-->
	<animation>
		<type>translate</type>
    		<object-name>backdoor_re_o</object-name>
		<object-name>backdoor_re_o.i</object-name>
		<object-name>backdoor_re_u</object-name>
		<object-name>backdoor_re_u.i</object-name>
		<object-name>backdoorglas_re</object-name>
<property>/sim/model/door-positions/ribackDoor/position-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>.1</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>0.9</dep>
			</entry>
		</interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
  
