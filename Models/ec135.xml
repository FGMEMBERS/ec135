			<alpha-prop>rotors/main/rpm</alpha-prop>
			<factor>0.002</factor>
			<offset>-0.45</offset>
			<max>0.4</max>
		</transparency>
		<threshold>0.001</threshold>
	</animation>
  
	<animation>
		<type>rotate</type>
		<object-name>rotor_disc_M</object-name>
